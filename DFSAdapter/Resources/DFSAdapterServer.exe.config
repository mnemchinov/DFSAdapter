<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="TextListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="DFSAdapterServer.log"/>
        <add name="EventListener" type="System.Diagnostics.EventLogTraceListener" initializeData="DFSAdapterServer"/>
      </listeners>
    </trace>
  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>
    <add key="strPort" value="8000" />
  </appSettings>
</configuration>

